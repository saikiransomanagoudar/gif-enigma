{
  "private": true,
  "name": "gif-enigma",
  "version": "0.0.0",
  "license": "BSD-3-Clause",
  "type": "module",
  "scripts": {
    "vite": "vite",
    "dev:client": "cross-env NODE_OPTIONS=--max-old-space-size=4096 vite build --config vite.config.client.js --watch",
    "dev:server": "cross-env NODE_OPTIONS=--max-old-space-size=4096 vite build --config vite.config.server.js --watch",
    "dev:devvit": "npx devvit playtest r/PlayGIFEnigma",
    "dev:devvit:test": "npx devvit playtest r/gif_enigma_dev7",
    "dev": "concurrently -p \"[{name}]\" -n \"DEVVIT,CLIENT,SERVER\" -c \"bgBlue.bold,bgMagenta.bold,bgGreen.bold\" \"npm run dev:devvit\" \"npm run dev:client\" \"npm run dev:server\" --restart-tries 30",
    "dev:test": "concurrently -p \"[{name}]\" -n \"DEVVIT,CLIENT,SERVER\" -c \"bgBlue.bold,bgMagenta.bold,bgGreen.bold\" \"npm run dev:devvit:test\" \"npm run dev:client\" \"npm run dev:server\" --restart-tries 30",
    "build:client": "cross-env NODE_OPTIONS=--max-old-space-size=4096 vite build --config vite.config.client.js",
    "build:server": "cross-env NODE_OPTIONS=--max-old-space-size=4096 vite build --config vite.config.server.js",
    "build": "npm run build:client && npm run build:server",
    "upload": "npx devvit upload",
    "install:prod": "npm run build && npx devvit install r/PlayGIFEnigma",
    "install:test": "npm run build && npx devvit install r/gif_enigma_dev7",
    "start": "npx devvit dev"
  },
  "dependencies": {
    "@devvit/public-api": "0.12.12",
    "@devvit/web": "0.12.12",
    "@google/generative-ai": "^0.24.0",
    "axios": "^1.8.3",
    "clsx": "^2.1.1",
    "express": "^5.2.1",
    "framer-motion": "^12.5.0",
    "ioredis": "^5.6.0",
    "motion": "^12.4.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.4.0",
    "tailwind-merge": "^3.0.2",
    "tsx": "^4.19.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.0.14",
    "@tailwindcss/vite": "^4.0.9",
    "@types/express": "^5.0.6",
    "@types/node": "^22.13.5",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "cross-env": "^10.1.0",
    "devvit": "0.12.12",
    "prettier": "^3.5.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "4.0.13",
    "typescript": "5.8.3",
    "vite": "^6.2.0",
    "vitest": "^3.0.7"
  }
}